<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <record id="view_order_form_inherit_maqabim" model="ir.ui.view">
        <field name="name">sale.order.form.maqabim_purchase</field>
        <field name="model">sale.order</field>
        <field name="inherit_id" ref="sale.view_order_form"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='order_line']/tree//field[@name='product_uom_qty']" position="after">
                <field name="print_qty"/>
            </xpath>
            <xpath expr="//field[@name='order_line']/form//label[@for='product_uom_qty']" position="before">
                <field name="print_qty"/>
            </xpath>
        </field>
    </record>
</odoo>