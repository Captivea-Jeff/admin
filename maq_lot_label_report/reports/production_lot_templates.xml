<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <template id="report_productionlotlabel">
        <t t-call="web.basic_layout">
            <div class="page" style="margin-right: -18px !important;">
                <t t-foreach="docs" t-as="doc">
                    <t t-foreach="range(lotid_by_product.get(str(doc.id), 0))" t-as="n">
                        <t t-call="maq_lot_label_report.label_lot_report">
                            <t t-set="product" t-value="doc.product_id"/>
                            <t t-set="lot_id" t-value="doc"/>
                        </t>
                    </t>
                </t>
            </div>
        </t>
    </template>
</odoo>
