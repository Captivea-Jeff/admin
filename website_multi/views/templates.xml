<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <template id="assets_backend" inherit_id="web.assets_backend">
        <xpath expr="." position="inside">
            <script type="text/javascript" src="/website_multi/static/src/js/website_dashboard.js"></script>
        </xpath>
    </template>

    <template id="assets_frontend" inherit_id="web.assets_frontend" name="Website Assets">
        <xpath expr="//script[last()]" position="after">
            <script type="text/javascript" src="/website_multi/static/src/js/website_root.js"></script>
        </xpath>
    </template>
</odoo>
