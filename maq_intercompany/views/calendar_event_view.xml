<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <!-- Added Company field to differentiate the calendar list view -->
	<record id="maq_calendar_event_form_inherit" model="ir.ui.view">
		<field name="name">maq.calendar.event.form.inherit</field>
		<field name="model">calendar.event</field>
		<field name="inherit_id" ref="calendar.view_calendar_event_form" />
		<field name="arch" type="xml">
			<xpath expr="//page[@name='page_misc']" position="inside">
				<group>
					<field name="company_id" options="{'no_create': True,'no_create_edit': True, 'no_open': True}"/>
				</group>
			</xpath>
		</field>
	</record>
</odoo>